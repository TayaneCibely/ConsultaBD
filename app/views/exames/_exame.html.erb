<div id="<%= dom_id exame %>">
  <p>
    <strong>"Código do Exame":</strong>
    <%= exame.cod %>
  </p>

  <p>
    <%= form.label :paciente_id %>
    <%= form.collection_select :paciente_id, Paciente.all, :id, :nome_completo, prompt: "Selecione um paciente" %>
  </p>

  <p>
    <strong>Data:</strong>
    <%= exame.data %>
  </p>

  <p>
    <strong>"Nome do Exame":</strong>
    <%= exame.nomeExame %>
  </p>

  <p>
    <strong>Descrição:</strong>
    <%= exame.descricao %>
  </p>

  <p>
    <strong>Resultado:</strong>
    <%= exame.resultado %>
  </p>

</div>
